{
  "kind": "collectionType",
  "collectionName": "bookings",
  "info": {
    "singularName": "booking",
    "pluralName": "bookings",
    "displayName": "Заказы",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "bookingStatus": {
      "type": "enumeration",
      "enum": ["На рассмотрении", "Подтвержден", "Оплачен", "Отменен"],
      "required": true
    },
    "paymentMethod": {
      "type": "enumeration",
      "enum": [
        "Наличка",
        "Наличка чек",
        "Банковская карта",
        "Сертификат",
        "Юр. лицо",
        "Яндекс оплата",
        "Оплата через агрегатор"
      ]
    },
    "cardNumber": {
      "type": "string",
      "private": true
    },
    "usePoints": {
      "type": "boolean",
      "default": false
    },
    "discount": {
      "type": "decimal"
    },
    "certificateNumber": {
      "type": "string"
    },
    "firstCall": {
      "type": "datetime"
    },
    "secondCall": {
      "type": "datetime"
    },
    "totalAmount": {
      "type": "decimal",
      "required": true
    },
    "bonusPoints": {
      "type": "integer",
      "default": 0
    },
    "customerName": {
      "type": "string",
      "required": true
    },
    "phoneNumber": {
      "type": "string",
      "required": true
    },
    "quest": {
      "type": "string",
      "required": true
    },
    "promoCode": {
      "type": "string"
    },
    "discountedPrice": {
      "type": "decimal"
    },
    "gameDateTime": {
      "type": "datetime",
      "required": true
    },
    "statusHistory": {
      "type": "json"
    }
  }
}
